              <ownedParameter xmi:id="_0GHkJQ8cEeyxqrp2_sTWng" name="n" type="_z99GoA8cEeyxqrp2_sTWng"/>
              <language>java</language>
              <body>int scelta;&#xD;
		System.out.println(&quot;Com'è orientato il collegamento? \n 0)&quot; + &#xD;
				l.getName() + &quot;---->&quot; + t.getName() + &quot;\n 1)&quot; +&#xD;
				t.getName() + &quot;---->&quot; + l.getName());&#xD;
		scelta = Utility.readLimitedInt(0, 1);&#xD;
		if (scelta == 0)&#xD;
			if (checkLinkExistence(l, t, n, 1))&#xD;
				System.out.println(LINK_GIA_PRESENTE);&#xD;
			else&#xD;
				n.addLink(new Link(l, t, n.getId(), 1));&#xD;
		else &#xD;
			if (checkLinkExistence(l, t, n, -1))&#xD;
				System.out.println(LINK_GIA_PRESENTE);&#xD;
			else {&#xD;
				n.addLink(new Link(l, t, n.getId(),-1));&#xD;
			}&#xD;
		//TODO metti qualche controllo sull'inserimento corretto dell'orientamento&#xD;
	</body>
