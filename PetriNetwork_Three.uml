				e1.printStackTrace();&#xD;
			}&#xD;
			for(String s : n) &#xD;
				networks.add((Network) ReadN.jsonToObject(s, Network.class));&#xD;
			for(String s : pn)&#xD;
				petriNetworks.add((Petri_network) ReadN.jsonToObject(s, Petri_network.class));&#xD;
	</body>
