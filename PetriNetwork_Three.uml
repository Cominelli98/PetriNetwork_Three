            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_0GEg0g8cEeyxqrp2_sTWng" name="startMenu" specification="_z9elgw8cEeyxqrp2_sTWng">
              <language>java</language>
              <body>loadSavedNets();&#xD;
		int select = -1;&#xD;
		do {&#xD;
			for (String s : MENUSTART) {&#xD;
				System.out.println(s);&#xD;
			}&#xD;
			&#xD;
			select = Utility.readLimitedInt(0, MENUSTART.length-4);&#xD;
			switch (select) {&#xD;
				case 1:&#xD;
					Menu_configuratore.configuratore();&#xD;
					break;&#xD;
				case 2:&#xD;
					Menu_fruitore.menuFriutore();&#xD;
					break;&#xD;
				case 0:&#xD;
					Utility.close();&#xD;
					break;&#xD;
			}&#xD;
		}while(select != 0);&#xD;
