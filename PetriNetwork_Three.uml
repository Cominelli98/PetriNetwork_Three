		try {&#xD;
			ArrayList&lt;String> nets = readFile(c);&#xD;
			Gson gson = new Gson();&#xD;
			for(String s : nets) {&#xD;
				IDNameGiver net = (IDNameGiver) gson.fromJson(s, c);&#xD;
				IDs.add(net.getId());&#xD;
			}&#xD;
		} catch (FileNotFoundException e) {&#xD;
			IDs.add(ID_INIZIALE);&#xD;
		}&#xD;
		return IDs;&#xD;
	</body>
