            <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="_0GEg0w8cEeyxqrp2_sTWng" name="loadSavedNets" specification="_z9fMkA8cEeyxqrp2_sTWng">
              <language>java</language>
              <body>ArrayList&lt;String> n = new ArrayList&lt;String>();&#xD;
